{
  "private": true,
  "name": "Sashi's personal blog",
  "description": "Musings written over time",
  "version": "0.0.0",
  "author": "Sashiraj Chandrasekaran",
  "license": "0BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby build",
    "develop": "nodemon --exec 'kill-port --port 8000 && gatsby develop' --watch gatsby-node.js --watch package.json --watch gatsby-config.js",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build && gh-pages -d public"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "^4.0.0",
    "gatsby": "^4.12.1",
    "gatsby-omni-font-loader": "^2.0.0",
    "gatsby-plugin-disqus": "^1.2.4",
    "gatsby-plugin-feed": "^4.12.1",
    "gatsby-plugin-gatsby-cloud": "^4.12.1",
    "gatsby-plugin-google-gtag": "^4.14.0",
    "gatsby-plugin-manifest": "^4.12.1",
    "gatsby-plugin-sitemap": "^5.12.1",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.27",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "gatsby-plugin-eslint": "^4.0.2",
    "gh-pages": "^4.0.0",
    "kill-port": "^2.0.0",
    "nodemon": "^2.0.16"
  }
}
